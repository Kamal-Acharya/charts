{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Welcome2",
    "definitions": {
        "Welcome2": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "database_credentials": {
                    "$ref": "#/definitions/DatabaseCredentials"
                },
                "image": {
                    "type": "string"
                },
                "migrations": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Migration"
                    }
                }
            },
            "required": [
                "database_credentials",
                "image",
                "migrations"
            ],
            "title": "Welcome2"
        },
        "DatabaseCredentials": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "env": {
                    "$ref": "#/definitions/DatabaseCredentialsEnv"
                },
                "envFrom": {
                    "$ref": "#/definitions/EnvFrom"
                }
            },
            "required": [],
            "title": "DatabaseCredentials"
        },
        "DatabaseCredentialsEnv": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DB_HOST": {
                    "type": "string"
                },
                "DB_PASSWORD": {
                    "type": "string"
                },
                "DB_PORT": {
                    "type": "string",
                    "format": "integer"
                },
                "DB_TYPE": {
                    "type": "string"
                }
            },
            "required": [],
            "title": "DatabaseCredentialsEnv"
        },
        "EnvFrom": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "secret": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [],
            "title": "EnvFrom"
        },
        "Migration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "enable": {
                    "type": "boolean"
                },
                "env": {
                    "$ref": "#/definitions/MigrationEnv"
                },
                "post_query": {
                    "type": "array",
                    "items": {}
                }
            },
            "required": [
                "enable"
            ],
            "title": "Migration"
        },
        "MigrationEnv": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "REPO_URL": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "REPO_NAME": {
                    "type": "string"
                },
                "BRANCH": {
                    "type": "string"
                },
                "COMMIT": {
                    "type": "string"
                },
                "SCRIPT_LOCATION": {
                    "type": "string"
                },
                "PARAM": {
                    "type": "string"
                },
                "MIGRATE_TO_VERSION": {
                    "type": "string",
                    "format": "integer"
                },
                "DB_NAME": {
                    "type": "string"
                }
            },
            "required": [
                "DB_NAME",
                "REPO_NAME",
                "REPO_URL"
            ],
            "title": "MigrationEnv"
        }
    }
}